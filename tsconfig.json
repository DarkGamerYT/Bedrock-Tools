{
  "include": [ "js/**/*", "types/**/*" ],
  "compilerOptions": {
    "allowJs": true,
    "checkJs": false,
    "module": "CommonJS",
    "target": "es2020",
    "strict": false,
    "noImplicitOverride": true,
    "noUncheckedIndexedAccess": true,
    "allowUnreachableCode": false,
    "noUnusedLocals": false,
    "noUnusedParameters": true,
    "skipLibCheck": true
  }
}