{
  "include": [ "types/**/*" ],
  "compilerOptions": {
    "allowJs": true,
    "checkJs": true,
    "module": "CommonJS",
    "target": "ES2020",
    "noEmit": true,
    "strict": false,
    "noImplicitOverride": true,
    "noPropertyAccessFromIndexSignature": true,
    "noUncheckedIndexedAccess": true,
    "allowUnreachableCode": false,
    "noUnusedLocals": true,
    "noUnusedParameters": true
  }
}